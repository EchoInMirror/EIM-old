<svg width="0" height="0" style="position: absolute">
  <defs>
    <pattern id="editor-grid" x="0" y="0" width="96" height="3240" patternUnits="userSpaceOnUse">
      <rect width="1" height="3240" fill="#bdbdbddd" x="0" y="0" />
      <rect width="0.6" height="3240" fill="#bdbdbd77" x="48" y="0" />
      <!-- <rect width="0.6" height="3240" fill="#bdbdbd77" x="192" y="0" />
      <rect width="0.6" height="3240" fill="#bdbdbd77" x="288" y="0" /> -->
    </pattern>
    <pattern id="editor-grid-y" x="0" y="0" width="80" height="294" patternUnits="userSpaceOnUse">
      <% for (let i = 0; i < 12; i++) { %>
        <rect width="80" height="0.5" fill="#bdbdbd77" x="0" y="<%= i * 24.5  %>"></rect>
      <% } %>
    </pattern>
  </defs>
</svg>

<template id="template-grid-y">
  <svg width="100%" height="2941">
    <rect fill="url(#editor-grid-y)" x="0" y="0" width="2496000" height="2941" />
  </svg>
</template>
